<div class="col-md-4">
  <div class="col-md-12 padding-0">
    <div class="panel box-v2">
      <div class="panel-heading padding-0">
        <img src="/img/bg2.jpg" class="box-v2-cover img-responsive" />
        <div class="box-v2-detail">
          <%  if(typeof userImg != 'undefined' && userImg) { %>
            <img src="/uploads/<%=userImg;%>" class="img-responsive" />
            <% } else {%>
              <img src="/img/avatar.jpg" class="img-responsive" />
              <% }%>

                <%  if(typeof userName != 'undefined' && userName) { %>
                      <h4><%=userName%>
                  <% } %>

                    </h4>
        </div>
      </div>
      <div class="panel-body">
        <div class="col-md-12 padding-0 text-center">
          <div class="col-md-4 col-sm-4 col-xs-6 padding-0">
            <h3>2.000</h3>
            <p>Post</p>
          </div>
          <div class="col-md-4 col-sm-4 col-xs-6 padding-0">
            <h3>2.232</h3>
            <p>share</p>
          </div>
          <div class="col-md-4 col-sm-4 col-xs-12 padding-0">
            <h3>4.320</h3>
            <p>photos</p>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="col-md-12 padding-0">
    <div class="panel box-v3">
      <div class="panel-heading bg-white border-none">
        <h4>Report</h4>
      </div>
      <div class="panel-body">
        <div class="media">
          <div class="media-left">
            <span class="icon-folder icons" style="font-size:2em;"></span>
          </div>
          <div class="media-body">
            <h5 class="media-heading">Document Handling</h5>
            <div class="progress progress-mini">
              <div class="progress-bar" role="progressbar" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100" style="width: 10%;">
                <span class="sr-only">60% Complete</span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>